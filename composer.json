{
    "name": "kirschbaum-development/eloquent-power-joins",
    "description": "The Laravel magic applied to joins.",
    "keywords": [
        "laravel",
        "eloquent",
        "mysql",
        "join"
    ],
    "homepage": "https://github.com/kirschbaum-development/eloquent-power-joins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Dalmolin",
            "email": "luis.nh@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*|4.*|5.*",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\EloquentJoins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirschbaumDevelopment\\EloquentJoins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\EloquentJoins\\EloquentJoinsServiceProvider"
            ]
        }
    }
}
