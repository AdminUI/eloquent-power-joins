{
    "name": "kirschbaum-development/laravel-where-has-with-joins",
    "description": "Laravel whereHas, but using joins.",
    "keywords": [
        "kirschbaum-development",
        "laravel-where-has-with-joins"
    ],
    "homepage": "https://github.com/kirschbaum-development/laravel-where-has-with-joins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Dalmolin",
            "email": "luis.nh@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\LaravelWhereHasWithJoins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirschbaumDevelopment\\LaravelWhereHasWithJoins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\LaravelWhereHasWithJoins\\LaravelWhereHasWithJoinsServiceProvider"
            ],
            "aliases": {
                "LaravelWhereHasWithJoins": "KirschbaumDevelopment\\LaravelWhereHasWithJoins\\LaravelWhereHasWithJoinsFacade"
            }
        }
    }
}
